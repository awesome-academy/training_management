<% @user_course.each do |course| %>
  <tr>
    <td class="col-xs-8 text-center"><%= link_to course.course_name, trainee_subjects_path %></td>
    <td class="col-xs-8 text-center"><%= l course.start_date, format: :date_month_year_concise %></td>
    <td class="col-xs-8 text-center"><%= l course.end_date, format: :date_month_year_concise %></td>
    <td class="col-xs-8 text-center"></td>
    <td class="col-xs-2 text-center">
      <%= link_to (t "admin.subjects.new.show"), trainee_subjects_path, class: "btn btn-info" %>
    </td>
  </tr>
<% end %>
